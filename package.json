{
  "name": "dream-market-web",
  "version": "1.0.0",
  "description": "The market of your dreams",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dream-Market/web.git"
  },
  "author": "Violet Kiseleva",
