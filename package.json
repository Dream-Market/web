    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.0",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1"
  },
  "devDependencies": {
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
