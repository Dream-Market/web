    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "prettier": "2.7.1",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
